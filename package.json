{
  "name": "rader-v3t",
  "version": "0.0.0",
  "description": "A template frontend using vite, vuetify 3 and vue 3",
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "test:unit": "vitest --environment jsdom",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx,.vue,.d.ts --ignore-pattern .gitignore",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "axios": "^0.24.0",
    "firebase": "^9.6.8",
    "pinia": "^2.0.12",
    "vue": "^3.2.6",
    "vue-router": "^4.0.12",
    "vuetify": "^3.0.0-beta.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.3.0",
    "sass": "^1.49.7",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.2.5",
    "vue-tsc": "^0.29.8"
  }
}
